{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    { "src": "api2/api.js", "use": "@vercel/node" },
    { "src": "api2/apii.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api-evm/(.*)", "dest": "api2/apii.js" },
    { "src": "/api-tron/(.*)", "dest": "api2/api.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
