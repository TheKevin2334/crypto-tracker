{
  "version": 2,
  "functions": {
    "api2/**/*.js": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "builds": [
    { "src": "api2/**/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api2/(.*)", "dest": "/api2/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
